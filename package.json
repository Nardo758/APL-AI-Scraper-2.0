{
  "name": "apl-ai-scraper-2.0",
  "version": "2.0.0",
  "description": "AI-Powered Visual Web Scraper with Command Center - Advanced scraping platform with GPT-4V integration and intelligent automation",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node node_modules/jest/bin/jest.js tests/unit/ --verbose --no-cache",
    "test:watch": "node node_modules/jest/bin/jest.js --watch",
    "test:integration": "node node_modules/jest/bin/jest.js tests/integration/ --verbose --no-cache",
    "test:all": "node node_modules/jest/bin/jest.js --verbose --no-cache",
    "test:stubbed": "node node_modules/jest/bin/jest.js --verbose --no-cache",
    "test:real": "cross-env SUPABASE_URL=$SUPABASE_URL SUPABASE_ANON_KEY=$SUPABASE_ANON_KEY node node_modules/jest/bin/jest.js --verbose",
    "test:junit": "node node_modules/jest/bin/jest.js --ci --reporters=default --reporters=jest-junit",
    "test:reports": "npm run test:junit && npm run test:integration",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "migrate": "node scripts/migrate.js",
    "worker": "node scripts/worker.js",
    "build": "npm run lint && npm test",
    "docker:build": "docker build -t apl-ai-scraper .",
    "docker:run": "docker run -p 3000:3000 apl-ai-scraper",
    "security": "npm audit --audit-level moderate",
    "coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "local-ci:windows": "powershell -NoProfile -ExecutionPolicy Bypass -File .\\local-ci.ps1",
    "local-ci:windows:stubbed": "powershell -NoProfile -ExecutionPolicy Bypass -File .\\local-ci.ps1 stubbed",
    "local-ci:windows:integration": "powershell -NoProfile -ExecutionPolicy Bypass -File .\\local-ci.ps1 integration",
    "db:verify": "node scripts/verify-database.js",
    "db:verify:simple": "node scripts/simple-db-verify.js",
    "db:verify:ps1": "powershell -File scripts/verify-schema.ps1",
    "db:setup": "echo 'Run scripts/setup-schema.sql in Supabase SQL Editor'",
    "db:test:pooled": "powershell -NoProfile -ExecutionPolicy Bypass -File .\\scripts\\run-test-pooled.ps1",
    "db:test": "npm run db:test:pooled",
    "db:apply-missing": "node scripts/apply-create-missing-tables.js",
    "db:apply-missing:yes": "node scripts/apply-create-missing-tables.js --yes",
    "local-ci": "powershell -NoProfile -ExecutionPolicy Bypass -File .\\local-ci.ps1"
  },
  "ci:scripts": {
    "ci:stubbed": "npm ci && npm run lint && npm test && npm run test:integration",
    "ci:integration": "npm ci && npm run test:all",
    "ci:security": "npm audit --audit-level moderate"
  },
  "keywords": [
    "web-scraping",
    "ai",
    "gpt-4v",
    "claude",
    "playwright",
    "automation",
    "data-extraction",
    "visual-scraping"
  ],
  "author": "APL Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nardo758/APL-AI-Scraper-2.0.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.13.1",
    "@supabase/supabase-js": "^2.38.5",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "bullmq": "^4.15.2",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-proxy-agent": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsdom": "^23.0.1",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "openai": "^4.24.0",
    "pg": "^8.16.3",
    "playwright": "^1.40.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-adblocker": "^2.13.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "rate-limiter-flexible": "^4.0.1",
    "socket.io": "^4.7.4",
    "socks-proxy-agent": "^8.0.2",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "babel-jest": "^29.7.0",
    "bcryptjs": "^3.0.2",
    "buffer-from": "^1.1.2",
    "coveralls": "^3.1.1",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.29.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-junit": "^13.0.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "otplib": "^12.0.1",
    "prettier": "^3.1.1",
    "redis": "^5.8.3",
    "robots-parser": "^3.0.1",
    "source-map": "^0.7.6",
    "supertest": "^6.3.3"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base"
    ],
    "env": {
      "node": true,
      "jest": true
    },
    "rules": {
      "no-console": "off",
      "consistent-return": "off",
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
